	  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
      <script type='text/javascript'>
            
            jQuery(document).ready(function($){
                
                $('.aw_subscribe').on('click',function(event){
                    event.preventDefault();
                    
                    var useremail = $('#useremail').val();
                    if (useremail == '') {
                         $('.aw_messages').html('<span class="error_message">Please Enter Email!</span');
                    }else{
                        
                    if ( validateEmail(useremail) ) {
                        
                    $.ajax({ 
                        url:'https://templatebundle.net/index.php/Newsletter/templatebundle_awbersubscribe',
                        method: 'post',
                       data: {useremail: useremail},
                       // dataType: 'json',
                        success: function(response){
                            
                           var result = JSON.parse(response);
                           if(result.statusf == 'success'){
                               
                            var cname = 'subscriber';
                            var cvalue = 'subscribealerday';
                            var exdays = 1;
                            var d = new Date(); 
                            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 10));
                            var expires = "expires="+d.toUTCString();
                            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
                            
                            $('.aw_messages').html('<span class="success_message">'+result.success+'</span');
                            
                            model_close();
                           
                            }else{
                                
                            $('.aw_messages').html('<span class="error_message">'+result.error+'</span');
                            
                           }
                        }
                        
                      });
                     // ajax end
                     
                     }else{
                       $('.aw_messages').html('<span class="error_message">Please Enter Validate Email Addresses!</span');  
                     }
                    
                    }
                    
                });
                
              function validateEmail(uemail) {
            	  var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            	    if (filter.test(uemail)) {
            	        return true;
            	    }
            	    else {
            	        return false;
            	    }
            	}   
                
            });
            
            function model_close(){
                setTimeout(function(){ $('#myModal').modal('hide'); }, 3000);
            }
            
           /**
                   * done show popup
                   */ 
                     
                   $('#checkboxdontshow').on('click',function() {
                       
                        if(this.checked) {
                            
                            var returnVal = confirm("Are you sure?");
                            
                            var checknow = $(this).prop("checked", returnVal);
                            
                            if(checknow){
                            var cname = 'nodshow';
                            var cvalue = 'subscribeformnodtshow';
                            var minutes = 30;
                            var d = new Date(); 
                            d.setTime(d.getTime() + (minutes * 60 * 1000));
                            var expires = "expires="+d.toUTCString();
                            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
                            
                            setTimeout(function(){ $('#myModal').modal('hide'); }, 100);
                            }
                            
                        }
                       
                   });
            
        </script>
